{
  "name": "nalsmobiletool",
  "displayName": "NALSMobileTool",
  "description": "Super power tool for NALS Mobile",
  "icon": "images/nals.png",
  "version": "2.2.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart",
    "workspaceContains:**/*.dart"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "NALSMobileTool settings",
      "properties": {
        "nalsMobileBrain.excludeFilesWhenAutoExport": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "A list of generated file extensions to exclude",
          "default": [
            "g.dart",
            "config.dart",
            "freezed.dart",
            "mapper.dart"
          ]
        },
        "nalsMobileBrain.autoExportOnSave": {
          "type": "boolean",
          "description": "Auto export on file save",
          "default": false
        },
        "nalsMobileBrain.autoExportBarrier": {
          "type": "string",
          "description": "Any code above the barrier will not be replaced. If it is empty, all codes will be replaced",
          "default": ""
        },
        "nalsMobileBrain.appName": {
          "type": "string",
          "description": "App name declared in pubspec.yaml",
          "default": "nalsflutter"
        },
        "nalsMobileBrain.uiFolderPath": {
          "type": "string",
          "description": "The folder path to create page and viewmodel",
          "default": "lib/ui/page"
        },
        "nalsMobileBrain.dataModelPath": {
          "type": "string",
          "description": "The folder path to create data model and mapper",
          "default": "lib/model/api"
        }
      }
    },
    "commands": [
      {
        "command": "nalsmobiletool.createNewPage",
        "title": "nals:Create new Page"
      },
      {
        "command": "nalsmobiletool.jsonToDataModel",
        "title": "nals:[API] Json to Data Model"
      },
      {
        "command": "nalsmobiletool.clipboardToDataModel",
        "title": "nals:[API] Clipboard to Data Model"
      },
      {
        "command": "nalsmobiletool.jsonToParams",
        "title": "nals:[API] Json to Params"
      },
      {
        "command": "nalsmobiletool.autoExport",
        "title": "nals:Auto export"
      },
      {
        "command": "nalsmobiletool.createUTFile",
        "title": "nals:Create test file"
      },
      {
        "command": "nalsmobiletool.extractApiUrl",
        "title": "nals:[API] Extract API URL"
      },
      {
        "command": "nalsmobiletool.translateAndExtractValueToArbFiles",
        "title": "nals: Translate and extract value to arb files"
      },{
        "command": "nalsmobiletool.sortArbFiles",
        "title": "nals: Sort arb files"
      }
    ],
    "menus": {
      "explorer/context": [],
      "commandPalette": []
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/view_model.json"
      },
      {
        "language": "dart",
        "path": "./snippets/others.json"
      },
      {
        "language": "dart",
        "path": "./snippets/ui.json"
      },
      {
        "language": "dart",
        "path": "./snippets/data_model.json"
      },
      {
        "language": "dart",
        "path": "./snippets/mapper.json"
      },
      {
        "language": "dart",
        "path": "./snippets/shared_pref.json"
      },
      {
        "language": "dart",
        "path": "./snippets/test.json"
      },
      {
        "language": "dart",
        "path": "./snippets/widget.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/json-to-ast": "^2.1.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "copy-paste": "^1.5.3",
    "googletrans": "^1.0.21",
    "json-to-ast": "^2.1.0",
    "jsonc": "^2.0.0",
    "lodash": "^4.17.19",
    "mkdirp": "^0.5.5",
    "pluralize": "^8.0.0",
    "yaml": "^2.7.0"
  }
}
